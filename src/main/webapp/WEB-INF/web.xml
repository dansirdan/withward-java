<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>
	<display-name>Servlet 3.0 Web Application</display-name>
	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<display-name>UserServlet</display-name>
		<description></description>
		<servlet-class>com.withward.servlets.UserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DestinationServlet</servlet-name>
		<display-name>DestinationServlet</display-name>
		<description></description>
		<servlet-class>com.withward.servlets.DestinationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WithlistServlet</servlet-name>
		<display-name>WithlistServlet</display-name>
		<description></description>
		<servlet-class>com.withward.servlets.WithlistServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>com.withward.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RatingServlet</servlet-name>
		<display-name>RatingServlet</display-name>
		<description></description>
		<servlet-class>com.withward.servlets.RatingServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WithlistUser</servlet-name>
		<display-name>WithlistUser</display-name>
		<description></description>
		<servlet-class>com.withward.servlets.WithlistUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/users/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DestinationServlet</servlet-name>
		<url-pattern>/destinations/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WithlistServlet</servlet-name>
		<url-pattern>/withlists/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RatingServlet</servlet-name>
		<url-pattern>/ratings/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WithlistUser</servlet-name>
		<url-pattern>/members/*</url-pattern>
	</servlet-mapping>
</web-app>